{
  "dashboard": {
    "id": null,
    "title": "QoderResume Load Testing Dashboard",
    "tags": ["load-testing", "k6", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Virtual Users",
        "type": "graph",
        "targets": [
          {
            "expr": "k6_vus",
            "refId": "A",
            "legendFormat": "Virtual Users"
          },
          {
            "expr": "k6_vus_max",
            "refId": "B", 
            "legendFormat": "Max VUs"
          }
        ],
        "yAxes": [
          {
            "label": "Users",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(k6_http_reqs_total[1m])",
            "refId": "A",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "req/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "k6_http_req_duration{quantile=\"0.5\"}",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "k6_http_req_duration{quantile=\"0.95\"}",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "k6_http_req_duration{quantile=\"0.99\"}",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (ms)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(k6_http_reqs_total{status!~\"2..\"}[1m]) / rate(k6_http_reqs_total[1m]) * 100",
            "refId": "A",
            "legendFormat": "Error Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Error %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "System Resources - CPU",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "System Resources - Memory", 
        "type": "graph",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "refId": "A",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "Memory %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "postgres_connections_active",
            "refId": "A",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "postgres_connections_max",
            "refId": "B",
            "legendFormat": "Max Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Redis Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "refId": "A",
            "legendFormat": "Memory Used"
          },
          {
            "expr": "redis_memory_max_bytes",
            "refId": "B",
            "legendFormat": "Max Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}