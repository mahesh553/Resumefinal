{
  "name": "qoder-resume",
  "version": "1.0.0",
  "description": "AI-powered resume optimization platform - Monolith Architecture",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev -p 3000",
    "dev:backend": "nest start --watch",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build",
    "build:backend": "nest build",
    "build:check": "npm run build:frontend",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "next start -p 3000",
    "start:backend": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "docker:dev": "cd infrastructure/docker && docker-compose up -d postgres redis",
    "docker:down": "cd infrastructure/docker && docker-compose down",
    "setup": "node scripts/setup.js || npm install --legacy-peer-deps",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/backend/database/migration-data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/backend/database/migration-data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/backend/database/migration-data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/backend/database/migration-data-source.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d src/backend/database/migration-data-source.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d src/backend/database/migration-data-source.ts",
    "db:optimize": "node scripts/db-optimize.js",
    "db:health": "node scripts/db-optimize.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.7.0",
    "@google/generative-ai": "^0.2.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^11.0.0",
